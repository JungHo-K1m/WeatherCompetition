<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:text="충청남도 농장"
        android:textAlignment="center"
        android:gravity="center_horizontal"
        android:textColor="@color/black"/>

    <Button
        android:id="@+id/cn_btn01"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="행복이있는정원농장"
        android:textSize="15dp"/>

    <Button
        android:id="@+id/cn_btn02"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="더 건강한 농원"
        android:textSize="15dp"/>

    <Button
        android:id="@+id/cn_btn03"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="오늘사과 예산농장"
        android:textSize="15dp"/>

    <Button
        android:id="@+id/cn_btn04"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="파인블루베리 농장"
        android:textSize="15dp"/>

    <Button
        android:id="@+id/cn_btn05"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="딸기마루"
        android:textSize="15dp"/>

    <Button
        android:id="@+id/cn_btn06"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="부여 딸기사랑 농원"
        android:textSize="15dp"/>

</LinearLayout>